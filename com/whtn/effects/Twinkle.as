package com.whtn.effects {	import com.whtn.Effect;	public class Twinkle extends Effect {		public function Twinkle ():void {			super("twinkle");			hotKey = "t";			speed = .25;			trail = 0;			brightness = 100;			//sequence = new Array(10);			for(i=0;i<8;i++) {				sequence.push(new Array());			}			/*for(i=0;i<sequence.length;i++){				if(sequence[i]==undefined)					sequence[i]=new Array();			}*/			for(i in sequence){				for(j=0;j<60;j+=3) {					sequence[i].push(new Array(j,picker(5,100)));				}			}		}	}}