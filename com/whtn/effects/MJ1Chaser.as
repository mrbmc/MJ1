package com.whtn.effects {	import com.whtn.Effect;	import com.whtn.TubeEvent;	import com.whtn.MJ1UI;	import com.whtn.Picker;	public class MJ1Chaser extends Effect {		public function MJ1Chaser ():void {			super("mj1_chaser");			device="mj1";			hotKey = "a";			trail = 0;			speed = 2;			var frames = 8;			var framesteps = 8;			var rot_inc = MJ1UI.MAX_ROTATION / framesteps;			for(i=0;i<frames;i++) {				var ii = i * 2;				for(j=0;j<framesteps;j++) {					var frame = new Array();					var rot = j * rot_inc;					var col = Picker.COLORS[j];					frame.push(new TubeEvent(i,100,rot,col));					sequence.push(frame);				}			}		}	}}