package com.whtn.effects {	import com.whtn.Effect;	public class ColorWash extends Effect {		public function ColorWash ():void {			super("colorwash");			target = "mj1";			hotKey = "a";			hasChannel = true;			sequence = new Array(16);			for(i=0,m=sequence.length;i<m;i++) {				//j=(i%12)*5;				//index = j+Math.floor(i/12);				var tube = Object();					tube.brightness = 100;					tube.color = 0xFF00FF					tube.rotation = 180;				sequence[i] = tube;			}		}	}}