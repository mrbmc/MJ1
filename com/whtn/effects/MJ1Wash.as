package com.whtn.effects {	import com.whtn.Effect;	import com.whtn.TubeSet;	public class MJ1Wash extends Effect {		public function MJ1Wash ():void {			super("mj1_wash");			device = "mj1";			hotKey = "W";			trail = 0;			speed = 4;			channel = false;			var display:Array = new Array(16);						for(frame=0;frame<16;frame++) {				display = [];				var ff = frame-8;				for(tube=0;tube<8;tube++) {					var bright = (tube <= frame && tube > ff) ? 100 : 0;					display[tube] = new TubeSet(frame,bright);					display[(tube+8)] = new TubeSet(frame,bright);				}				sequence.push(display);			}			display = [];			for(i=0;i<16;i++) display.push(new TubeSet());			sequence.push(display);		}//constructor	}}