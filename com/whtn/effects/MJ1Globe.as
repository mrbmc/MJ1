package com.whtn.effects {	import com.whtn.Effect;	import com.whtn.TubeEvent;	import com.whtn.TubeSet;	import com.whtn.MJ1UI;	import com.whtn.Picker;	public class MJ1Globe extends Effect {		public function MJ1Globe ():void {			super("mj1_globe");			device="mj1";			hotKey = "U";			trail = 0;			speed = 2;			channel = false;			var frames = 8;			var framesteps = 4;			var rot_inc = Math.ceil(MJ1UI.MAX_ROTATION / (framesteps-1));			var col = null;			var display:Array = [];			for(i=0;i<16;i++) display.push(new TubeSet(i,0));			sequence.push(display);			for(frame=0;frame<16;frame++) {				display = [];				for(tube=0;tube<16;tube++) {						display[tube] = new TubeSet(frame,0,-1);//						display[(tube+8)] = new TubeSet(frame,0,-1);				}				display[frame].brightness = 100;
				var f = frame -1;
				if(f<0) f = 15;//				display[f].brightness = 10;//				display[(frame-2)].brightness = 25;				sequence.push(display);//				trace(sequence[frame][0].brightness);			}			display = [];			for(i=0;i<16;i++) display.push(new TubeSet(i,0));			sequence.push(display);		}//constructor	}//class}//package