package com.whtn.effects {	import com.whtn.Effect;	public class Strobe extends Effect {		public function Strobe ():void {			super("strobe");			target = "mj1";			speed = 2;			hasChannel = false;			a = new Array();b = new Array();			for(i=0;i<60;i++) a.push(i);			for(i=0;i<32;i++) {				if(i%2==0) {					sequence.push(a);				} else {					sequence.push(b);				}			}		}	}}