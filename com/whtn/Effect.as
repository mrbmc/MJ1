package com.whtn {	import flash.events.EventDispatcher;	import flash.events.Event;	public class Effect extends EventDispatcher {		public var auto:Boolean = true;		public var loops:int = 0;		public var reverse:Boolean = false;		public var reflect:Boolean = false;		public var sequence:Array = new Array();		public var speed:Number = 1;		public var trail:int = 0;		public var brightness:int = 100;		public var channel:Boolean = true;		public var name:String = null;		public var device:String = "sn1"; 		public var hotKey:String = null;		private var a = new Array();		private var b = new Array();		public function Effect(_effectName:String=null) {			this.name = _effectName;		}		// Chooses a random number between the min & max val		protected function picker(min=0,max=0):int {			return (Math.round(Math.random()*(max-min))+min);		}		// Limits the input value between the min and max		protected function confine(val,min,max) {			if(val>max)				val -= max;			else if(val<min)				val += max;			return val;		}		public function go(o:*=null) {			dispatchEvent(new EffectEvent(EffectEvent.PLAY,this));		}	}}