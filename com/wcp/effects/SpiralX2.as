package com.wcp.effects {	import com.wcp.Effect;	public class SpiralX2 extends Effect {		public function SpiralX2 ():void {			super("spiralx2");			hotKey = "r";			trail = 3;			sequence = new Array();			a = new Array(30,35,40,45,50,55,0 ,5 ,10,15,20,25);			for(i=0;i<60;i++) {				index = ((i%12)*5)+Math.floor(i/12);				beam = (i%12);				ring = (i%5);				v = a[beam] + Math.floor(i/12);				sequence.push(new Array(index,v));			}		}	}}