package com.wcp.effects {	import com.wcp.Effect;	public class Propeller extends Effect {		public function Propeller ():void {			super("propeller");			trail = 1;			loops = 3;			sequence = new Array(6);            for(i=0;i<60;i++) {                beam = (i%12);                frame = (i%6);                ring = (i%5);                if(sequence[frame]==null)                    sequence[frame] = new Array();                sequence[frame].push(i);            }		}	}}