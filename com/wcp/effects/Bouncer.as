package com.wcp.effects {	import com.wcp.Effect;	public class Bouncer extends Effect {		public function Bouncer ():void {			super("bouncer");			hotKey = "e";			speed = 2;			sequence = new Array(60);			for(i=0;i<60;i++) {				arm = (i % 13) * 5;				ring = Math.floor(i / 13);				index = arm+ring;				sequence[i] = new Array( index + "" );			}			for(i=0;i<60;i++) {				arm = this.confine(60-((i%13)*5),0,59);				ring = Math.floor(i/13);				index = arm + ring;				sequence[i].push(index+"");			}		}	}}