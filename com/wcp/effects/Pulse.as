package com.wcp.effects {	import com.wcp.Effect;	public class Pulse extends Effect {		public function Pulse ():void {			super("pulse");			speed = 2;			brightness = 50;			var j = v = 5;			//v=5;			while(v<100) {				a = new Array();b = new Array();				for(i=0;i<60;i++) {					a.push(new Array(""+i,v));				}				sequence.push(a);				v = Math.min( Math.pow(j,(j*0.05)),100);				j++;			}			v=100;			j=1;			while(v>5) {				a = new Array();b = new Array();				for(i=0;i<60;i++) {					a.push(new Array(""+i,v));				}				sequence.push(a);				v -= j*(100/60);				j++;			}		}	}}